  background-image: -webkit-linear-gradient(#0c1d38, #1a3158);
  background-image: linear-gradient(#0c1d38, #1a3158);
  position: relative;
  width: 364rpx;
  height: 364rpx;
  margin: 300rpx auto;
  overflow: hidden;
  -webkit-mask-image: -webkit-radial-gradient(circle, white, black);
}
#christmas .ground {
  border-top-right-radius: 260rpx 80rpx;
